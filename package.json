{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "online store to make their great product ideas available for purchase",
  "main": "server.js",
  "scripts": {
    "start": "npx tsc && node ./dist/server.js",
    "dev": "nodemon ./src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "jasmine-ts",
    "tsc": "tsc",
    "format": "prettier --write src/**/*.ts",
    "lint:fix": "eslint --fix",
    "lint": "eslint . --ext .ts",
    "migration:run": "db-migrate up",
    "migrate:test": "db-migrate --env test up && db-migrate up"
  },
  "author": "@MmFerry",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jasmine": "^4.5.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.20",
    "pg": "^8.9.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0"
  }
}
