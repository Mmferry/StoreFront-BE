{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "online store to make their great product ideas available for purchase",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "jasmine-ts",
    "tsc": "tsc",
    "format": "prettier --write src/**/*.ts",
    "lint:fix": "eslint --fix",
    "lint": "eslint . --ext .ts",
    "migration:run": "db-migrate up",
    "migrate:test": "db-migrate --env test up && db-migrate up"
  },
  "author": "@MmFerry",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "2.8.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
